<script>
    function getSubCategoryData(){
        var catId = document.getElementById('catId').value;
        $.ajax({
            url : "/admin/extraCategory/getSubcatData", 
            method : "post",
            data : {
                cateGoryId : catId
            },
            success : function(res){
                $('#subcatId').html(res)
                $('#subcatId').removeAttr('disabled',"disabled");
            }
        })
    }

    function getExCategoryData(){
        var catId = document.getElementById('catId').value;
        var subcatId = document.getElementById('subcatId').value;
        $.ajax({
            url : "/admin/brand/getExcatData",
            method : "post",
            data : {
                cateGoryId : catId,
                subcateGoryId : subcatId
            },
            success : function(res){
                $('#excatId').html(res)
                $('#excatId').removeAttr('disabled',"disabled");
            }
        })
    }

    function getBrandData(){
        var catId = document.getElementById('catId').value;
        var subcatId = document.getElementById('subcatId').value;
        var excatId = document.getElementById('excatId').value;
        $.ajax({
            url : "/admin/type/getBrData",
            method : "post",
            data : {
                cateGoryId : catId,
                subcateGoryId : subcatId,
                extracateGoryId : excatId
            },
            success : function(res){
                $('#brandId').html(res)
                $('#brandId').removeAttr('disabled',"disabled");
            }
        })
    }

    function getTypeData(){
        var catId = document.getElementById('catId').value;
        var subcatId = document.getElementById('subcatId').value;
        var excatId = document.getElementById('excatId').value;
        var brandId = document.getElementById('brandId').value;
        $.ajax({
            url : "/admin/product/getTyData",
            method : "post",
            data : {
                cateGoryId : catId,
                subcateGoryId : subcatId,
                extracateGoryId : excatId,
                BrandId : brandId
            },
            success : function(res){
                $("#typeId").html(res);
                $("#typeId").removeAttr('disabled',"disabled");
            }
        })
    }
    function deleteimg(id,i,img){
        console.log(id);
        console.log(i);
        $.ajax({
            url:"/admin/product/deleteimg",
            method:"post",
            data:{
            'id':id,
            'i':i,
            'img':img
            },
            success:function(res){
            location.reload();
            }
        })
    }
</script>
