<%-include('header')%>
       <!-- Form Start -->
       <div class="container-fluid pt-4 px-4">
        <div class="row g-4">
            <div class="col-6">
                <div class="bg-secondary rounded h-100 p-4">
                    <h6 class="mb-4">Update Category</h6>
                    <form method="post" action="/admin/category/editCategoryData">
                        <input type="hidden" name="EditId" value="<%=CategoryData.id%>">
                        <div class="row mb-3">
                            <label for="inputEmail3" class="col-sm-2 col-form-label">Category</label>
                            <div class="col-sm-10">
                                <input type="text" name="category" value="<%=CategoryData.category%>" class="form-control" id="inputEmail3" placeholder="Enter Category">
                            </div>
                        </div>
                        <button type="submit" class="btn btn-primary">Edit</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- Form End -->
<%-include('footer')%>